function initMap(){
	var options = {
	 	types: ['(cities)'],
	 	componentRestrictions: {country: 'in'}
	};
	var input =  document.getElementById('from_location');
	var autocomplete = new google.maps.places.Autocomplete(input,options);
	autocomplete.addListener('place_changed', function() {
		var place = autocomplete.getPlace();
	    if (!place.geometry) {
	      window.alert("Autocomplete's returned place contains no geometry");
	      return;
	    }
	    $("#load_from_lat").val(place.geometry.location.lat());
	    $("#load_from_lng").val(place.geometry.location.lng());
	});
	var input1 =  document.getElementById('to_location');
	var autocomplete1 = new google.maps.places.Autocomplete(input1,options);
	autocomplete1.addListener('place_changed', function() {
		var place = autocomplete1.getPlace();
	    if (!place.geometry) {
	      window.alert("Autocomplete's returned place contains no geometry");
	      return;
	    }
	    $("#load_to_lat").val(place.geometry.location.lat());
	    $("#load_to_lng").val(place.geometry.location.lng());
	});
	var truck_from_location =  document.getElementById('truck_from_location');
	var autocomplete3 = new google.maps.places.Autocomplete(truck_from_location,options);
	var truck_to_location =  document.getElementById('truck_to_location');
	var autocomplete4 = new google.maps.places.Autocomplete(truck_to_location,options);
}

