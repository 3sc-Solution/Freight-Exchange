	var count=0;
	function registration_datechange()
	{
		var a=document.getElementById("shipdateload").value;
		var d = new Date();
		today1=d.setDate(d.getDate() - 15);
		if((d.getMonth() + 1)<=9)
		{
			var today='0'+(d.getMonth() + 1) + '/' + d.getDate() + '/' + d.getFullYear();
		}
		else
		var today=(d.getMonth() + 1) + '/' + d.getDate() + '/' + d.getFullYear();
		today=today.toString(); 
		a=new Date(a).getTime()
		today = new Date(today).getTime();
		if (a<today)
		{
			count++;
			document.getElementById("registration_datelabel").innerHTML="Availability date cannot be earlier than 2 weeks.";
			document.getElementById("submitload").disabled = true;
		}
		else
		{
			document.getElementById("registration_datelabel").innerHTML="";
			document.getElementById("submitload").disabled = false;
		}
		
	}
